    "test:node": "node node_modules/istanbul/lib/cli.js cover tests/js/all-tests.js --report none --print none",
    "posttest:cleanup": "node tests/js/pouchdb-express-tests/final-cleanup.js",
    "posttest:report": "node node_modules/istanbul/lib/cli.js report --root coverage --dir reports text-summary html",
    "posttest": "npm run posttest:cleanup && npm run posttest:report",
